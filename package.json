  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/react": "^15.0.16",
    "@types/react-dates": "^7.0.1",
    "@types/react-dom": "^0.14.23",
    "@types/request": "0.0.41",
    "@types/underscore": "^1.8.0",
    "axios": "^0.15.3",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "express": "^4.15.2",
    "moment": "^2.17.1",
    "mysql": "^2.11.1",
    "nodemon": "^1.10.2",
    "pubnub": "^4.5.0",
    "pug": "^2.0.0-beta11",
    "querystring": "^0.2.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dates": "^9.0.0",
    "react-dom": "^15.4.2",
    "request": "^2.75.0",
    "ts-node": "^3.0.2",
    "underscore": "^1.8.3",
    "webpack": "^2.2.1"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run watch-js\" \"npm run watch-css\"",
    "build": "npm run build-js && npm run build-css",
    "build-js": "webpack -p",
    "watch-js": "webpack -d --watch",
    "build-css": "node-sass --include-path src/scss src/scss/main.scss src/public/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.10",
    "awesome-typescript-loader": "^3.1.2",
    "concurrently": "^3.4.0",
    "node-sass": "^4.5.2",
    "source-map-loader": "^0.2.0",
    "typescript": "^2.2.1"
  }
}
